material chillskybox_m
{
	technique
	{
		pass
		{
			fog_override true
			lighting off
			depth_write off
			diffuse 1 1 1 1
			ambient 1 1 1 1

			texture_unit
			{
				cubic_texture chillsky_m_front.dds chillsky_m_back.dds chillsky_m_left.dds chillsky_m_right.dds chillsky_m_top.dds chillsky_bottom.dds separateUV
				tex_address_mode clamp
			}
		}
	}
}
material cabin_cabin_shiny
{
	technique
	{
		pass
		{
			lighting off

			ambient 0 0 0 1
