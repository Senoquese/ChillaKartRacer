material trees_tree
{
	technique
	{
		pass
		{
			lighting off

			ambient 0 0 0 1
			diffuse 1 1 1 1
			specular 0 0 0 1
			emissive 0 0 0
			cull_hardware none
      			cull_software none

			texture_unit
			{
				texture branches.dds
				tex_coord_set 0
				colour_op modulate
				scale 1 1
				scroll 0 0
