material water
{
  technique
  {
    pass
    {
	
      scene_blend alpha_blend
      depth_write off
	cull_hardware none
      cull_software none 
	lighting off
      
      texture_unit
      {
        colour_op_ex source1 src_manual src_current .2 .64 1
	  alpha_op_ex source1 src_manual src_current 0.5
      }

      texture_unit
      {
        texture ice.dds
        colour_op_ex modulate_x2 src_current src_texture 
        env_map spherical
      }





    }
  }
}
