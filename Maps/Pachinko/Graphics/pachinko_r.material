material glass
{
  technique
  {
    pass
    {
      scene_blend alpha_blend
      depth_write off
	cull_hardware none
        cull_software none 
	lighting on
	ambient 0 0 0 1
	diffuse 1 1 1 1
	specular 0 0 0 1
	emissive 0 0 0

			texture_unit
			{
				texture falloutglass.dds
				tex_coord_set 0
				colour_op modulate
				scale 1 1
				scroll 0 0
				rotate 0
			}

//metal map
			texture_unit
			{
				texture glasscubemap2.dds
				env_map spherical
				colour_op add
			}


    }
  }
}

